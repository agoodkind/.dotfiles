Include ~/.dotfiles/.ssh_config.local

# Read more about SSH config files: <https://linux.die.net/man/5/ssh_config>
Host home
    User agoodkind
    HostName home.goodkind.io

Host home router
    User agoodkind
    HostName router.home.goodkind.io

Host suburban
    User agoodkind
    HostName suburban.goodkind.io
    ForwardAgent yes
    LocalForward 10000 localhost:10000

Host mom
    HostName 10.240.0.121 # this might change TODO check
    User julia
    ProxyJump suburban

Host mini
    HostName mini.home.goodkind.io
    User agoodkind

Host jetkvm
    User root
    HostName jetkvm.home.goodkind.io
    ForwardAgent yes
    LocalForward 80 localhost:8080
    LocalForward 443 localhost:8443
    ProxyJump mini

Host berylax
    HostName berylax.mobile.goodkind.io
    User root
    ForwardAgent yes
