Include ~/.dotfiles/.ssh_config.local

# Read more about SSH config files: <https://linux.die.net/man/5/ssh_config>
Host home router
    User agoodkind
    HostName edge.1335-sf.goodkind.io

Host suburban
    User agoodkind
    HostName 128-nj.goodkind.io
    ForwardAgent yes
    LocalForward 10000 localhost:10000

Host mom
    HostName mom.lan.128-nj.goodkind.io # this might change TODO check
    User julia
    ProxyJump suburban

Host mini
    HostName mini.lan.1335-sf.goodkind.io
    User agoodkind

Host jetkvm
    User root
    HostName jetkvm.lan.1335-sf.goodkind.io
    ForwardAgent yes
    LocalForward 80 localhost:8080
    LocalForward 443 localhost:8443
    ProxyJump mini

Host berylax
    HostName berylax.mobile.goodkind.io
    User root
    ForwardAgent yes
