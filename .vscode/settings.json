{
    "files.associations": {
        "*.zsh": "shellscript",
        ".zshrc.*": "shellscript",
        "${workspaceFolder}/home/.ssh/config": "ssh_config",
        "${workspaceFolder}/.ssh_config.local": "ssh_config",
        "${workspaceFolder}/lib/.gitconfig_incl": "gitconfig"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/CVS": true,
        "**/.ruby-lsp": true,
        "**/.retool_types/**": true,
        "**/*tsconfig.json": true,
        ".cache": true,
        "retool.config.json": true
    },
    "editor.rulers": [
        90
    ],
}